{
  "name": "nrun",
  "version": "1.0.0",
  "description": "Alternative package.json scripts runner for node.js",
  "scripts": {
    "lint": "eslint ./",
    "test": "jest",
    "testLs": "ls",
    "spellcheck": "yaspeller-ci *.md",
    "testShowPath": "echo $PATH",
    "testShowPathWin": "echo %PATH%",
    "pretestEcho": "echo prescript",
    "testEcho": "echo script",
    "posttestEcho": "echo postscript"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/2do2go/nrun.git"
  },
  "bin": {
    "nrun": "./bin/nrun.js"
  },
  "keywords": [
    "scripts",
    "package.json",
    "runner",
    "run"
  ],
  "author": "2do2go team <dev.2do2go@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/2do2go/nrun/issues"
  },
  "homepage": "https://github.com/2do2go/nrun#readme",
  "devDependencies": {
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.0",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.7",
    "yaspeller-ci": "^1.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.md": "yaspeller-ci"
  },
  "yaspeller": {
    "dictionary": [
      "nrun",
      "js"
    ]
  }
}
